<p id="notice"><%= notice %></p>

<h2>投稿一覧</h2>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= feed.image %></td>
        <td><%= feed.title %></td>
        <td><%= feed.content %></td>
        <td><%= link_to 'Show', feed %></td>
        <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
        <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p><%= link_to '新規投稿へ', new_feed_path %></P>
<p><%= link_to 'ログイン画面へ（試行錯誤）', new_user_path %></P>
